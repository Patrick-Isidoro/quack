%%describe
Supports formatting lambda expressions with expressions and statements
%%source
do &x -> x              -- Identity
do &[x] -> x            -- Identity with parenthesis
do &[a, b] -> a         -- Binary
do &[] -> 1             -- Unit
do &[name] -> begin
  do debug.trace( name )
end
%%expect
do &x -> x
do &[x] -> x
do &[a, b] -> a
do &[] -> 1
do &[name] -> begin
  do debug.trace(name)
end
