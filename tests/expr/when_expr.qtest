%%describe
Supports formatting when expressions
%%source
fn mySwitch(what :: string)
  ^ when
         | what = "Luiz"      -> "Turing Tape!";
       | what = "Marcelo" -> "Lambda Calculus!";
     | else ""
    end
end
%%expect
fn mySwitch(what :: string)
  ^ when
    | what = "Luiz" -> "Turing Tape!";
    | what = "Marcelo" -> "Lambda Calculus!";
    | else ""
  end
end
